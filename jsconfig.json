{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "utils/*": ["utils/*"],
      "graphQL/*": ["graphQL/*"],
      "assets/*": ["assets/*"],
      "redux/*": ["redux/*"],
      "css-variable/*": ["css-variable/*"],
      "pages/*": ["pages/*"],
      "components/*": ["components/*"],
      "custom-hooks/*": ["custom-hooks/*"]
    },
    "module": "commonjs",
    "target": "es6",
    "jsx": "react"
  },
  "exclude": ["node_modules"],
  "include": ["src/**/*"]
}